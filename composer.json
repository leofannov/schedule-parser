{
    "name": "rasp/schedule-parser",
    "description": "Парсер расписания из XLSX с темами и автообновлением",
    "type": "project",
    "require": {
        "phpoffice/phpspreadsheet": "^1.29",
        "ext-zip": "*",
        "ext-xml": "*",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}